{
  "name": "@ladle/core",
  "version": "2.6.3",
  "repository": "git@github.com:tajo/ladle.git",
  "author": "Vojtech Miksu <vojtech@miksu.cz>",
  "license": "MIT",
  "type": "module",
  "types": "./lib/app/exports.ts",
  "exports": {
    ".": "./lib/app/exports.ts",
    "./serve": "./api/serve.js",
    "./preview": "./api/preview.js",
    "./build": "./api/build.js",
    "./framework": "./lib/cli/framework.js",
    "./shared/types": "./lib/shared/types.ts",
    "./shared/default-config": "./lib/shared/default-config.js"
  },
  "typesVersions": {
    "*": {
      "framework": [
        "typings-for-build/cli/framework.d.ts"
      ],
      "shared/types": [
        "./lib/shared/types.ts"
      ],
      "shared/default-config": [
        "typings-for-build/shared/default-config.d.ts"
      ]
    }
  },
  "packageManager": "pnpm@7.26.3",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "api",
    "typings-for-build"
  ],
  "bin": {
    "ladle": "./lib/cli/cli.js"
  },
  "scripts": {
    "cli": "node ./lib/cli/cli.js",
    "clean": "rimraf dist && rimraf .ladle && rimraf build && rimraf *.tsbuildinfo",
    "serve": "node ./lib/cli/cli.js serve",
    "types": "tsc --project tsconfig.typesoutput.json && cp ./typings-for-build/app/exports.d.ts ./typings-for-build/app/exports.d.cts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.2.1",
    "boxen": "^7.0.1",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "commander": "^10.0.0",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.4",
    "default-browser": "^3.1.0",
    "express": "^4.18.2",
    "get-port": "^6.1.2",
    "globby": "^13.1.3",
    "history": "^5.3.0",
    "lodash.merge": "^4.6.2",
    "open": "^8.4.0",
    "query-string": "^8.1.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "source-map": "^0.7.4",
    "vfile": "^5.3.6",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.18.6",
    "@babel/core": "^7.20.12",
    "@babel/generator": "^7.20.14",
    "@babel/parser": "^7.20.15",
    "@babel/template": "^7.20.7",
    "@babel/traverse": "^7.20.13",
    "@babel/types": "^7.20.7",
    "@babel/cli": "^7.20.7",
    "@types/babel__code-frame": "^7.0.3",
    "@types/babel__core": "^7.20.0",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__template": "^7.4.1",
    "@types/babel__traverse": "^7.18.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.11.19",
    "@types/ws": "^8.5.4",
    "cross-env": "^7.0.3",
    "vitest": "^0.28.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
